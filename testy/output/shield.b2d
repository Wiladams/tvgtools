BLImage render(const BLContextCreateInfo & cci) {
  BLImage img(24, 24, BL_FORMAT_PRGB32);
  BLContext ctx(img, cci);
  ctx.clearAll();
  ctx.setFillStyle(BLRgba32(0xFFffffff));
  ctx.fillAll();

  BLPoint vtxOut;

  do {
BLPath segment;
segment.moveTo(12.00, 1.00);
segment.lineTo(3.00,5.00);
// vlineto
segment.getLastVertex(&vtxOut);
segment.lineTo(vtxOut.x, 11.00);
segment.cubicTo(3.00,16.50,6.75,21.75,12.00,23.00);
segment.cubicTo(17.25,21.75,21.00,16.50,21.00,11.00);
// vlineto
segment.getLastVertex(&vtxOut);
segment.lineTo(vtxOut.x, 5.00);
segment.moveTo(17.25, 17.00);
segment.cubicTo(16.00,18.75,14.00,20.25,12.00,21.00);
segment.cubicTo(10.00,20.25,8.00,18.75,6.75,17.00);
segment.cubicTo(6.50,16.50,6.25,16.00,6.00,15.50);
segment.cubicTo(6.00,13.75,8.75,12.50,12.00,12.50);
segment.cubicTo(15.25,12.50,18.00,13.75,18.00,15.50);
segment.cubicTo(17.75,16.00,17.50,16.50,17.25,17.00);
segment.moveTo(12.00, 5.00);
segment.cubicTo(13.50,5.00,15.00,6.25,15.00,8.00);
segment.cubicTo(15.00,9.50,13.75,11.00,12.00,11.00);
segment.cubicTo(10.50,11.00,9.00,9.75,9.00,8.00);
segment.cubicTo(9.00,6.50,10.25,5.00,12.00,5.00);
ctx.setFillStyle(BLRgba32(41,173,255,255));
    ctx.fillPath(segment);
ctx.setFillStyle(BLRgba32(41,173,255,255));
    ctx.fillPath(segment);
  ctx.setStrokeWidth(0.00);
ctx.setStrokeStyle(BLRgba32(0,0,0,0));
  ctx.strokePath(segment);
} while(false);
  return img;
}
